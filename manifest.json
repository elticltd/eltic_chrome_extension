{
  "manifest_version": 3,
  "name": "ELTic - Job Change Prediction",
  "version": "1.1",
  "description": "Help recruiters, talent acquisition professionals, and networkers by predicting the likelihood of a LinkedIn user changing jobs",
  "permissions": ["activeTab", "storage", "tabs", "alarms", "identity", "windows"],
  "host_permissions": ["https://www.linkedin.com/*", "https://eltic.io/*"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "48": "images/favicon.png",
    "128": "images/favicon-192x192.png",
    "250": "images/favicon-512x512.png"
  },
  "web_accessible_resources": [
    {
      "resources": [
        "popup.html",
        "popup.js",
        "login.html",
        "login.js",
        "settings.html",
        "settings.js",
        "styles.css",
        "watchlist.html",
        "watchlist.js",
        "register.html",
        "register.js",
        "bulkupload.html",
        "bulkupload.js"
      ],
      "matches": ["https://www.linkedin.com/*", "https://eltic.io/*"]
    }
  ],
  "oauth2": {
    "client_id": "407445501282-efuiuab8gamac2o82avebm159456icsm.apps.googleusercontent.com",
    "extention_id": "oljadpnbbjmlkcgagcaomodejcoblohh",
    "scopes": [
      "https://www.googleapis.com/auth/userinfo.email",
      "https://www.googleapis.com/auth/userinfo.profile"
    ]
  },
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/*"],
      "js": ["content.js"]
    }
  ]
}
